
rem //////////// Only edit if you know what you are doing
rem Codecs for video
set video-codec=248/137/398/247
rem Codecs for audio
set audio-codec=140/m4a
rem Video encoding preferences
set video-pref=
rem Audio encoding - normalization preferences
set audio-encode=-acodec libmp3lame -aq 2
rem Set if you want to convert your m4a audio files to mp3, true for YES, false for no
set audioconverting=false
rem //////////// Feel free to edit this section
rem Download location - EDIT - Example D:\mymusic
rem Default location is MEDIA, root folder of the tool
set dllocation=media


